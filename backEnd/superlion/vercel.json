{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/test",
            "dest": "/test"
        },
        {
            "src": "/string",
            "dest": "/string"
        },
        {
            "src": "/json",
            "dest": "/json"
        },
        {
            "src": "/articleList",
            "dest": "/articleList"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}